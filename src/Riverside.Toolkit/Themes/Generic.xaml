    <Style TargetType="local2:TitleBarEx">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local2:TitleBarEx">
                    <Grid
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}">
                        <Button Visibility="Collapsed">
                            <Button.Flyout>
                                <MenuFlyout x:Name="CustomRightClickFlyout">
                                    <MenuFlyoutItem Text="Close"/>
                                    <MenuFlyoutItem Text="Close"/>
                                    <MenuFlyoutItem Text="Close"/>
                                    <MenuFlyoutItem Text="Close"/>
                                    <MenuFlyoutItem Text="Close"/>
                                    <MenuFlyoutItem Text="Close"/>
                                </MenuFlyout>
                            </Button.Flyout>
                        </Button>
                        <Border x:Name="AccentStrip" Height="31" VerticalAlignment="Top" Background="{ThemeResource SystemAccentColor}" Visibility="Collapsed" />
                        <Image x:Name="TitleBarIcon" Width="16" Margin="8,7" HorizontalAlignment="Left" VerticalAlignment="Top" x:FieldModifier="public"/>
                        <StackPanel Margin="29,7,140,0" HorizontalAlignment="Left" VerticalAlignment="Top" Orientation="Horizontal">
                            <TextBlock x:Name="TitleTextBlock" FontFamily="Segoe UI" Foreground="{TemplateBinding CurrentForeground}" FontSize="12" FontWeight="Normal" TextTrimming="CharacterEllipsis" Text="{TemplateBinding Title}"/>
                            <TextBlock FontFamily="Segoe UI" FontSize="12" FontWeight="Normal" Foreground="{TemplateBinding CurrentForeground}" Opacity="0.5" Text="{TemplateBinding Subtitle}" TextTrimming="CharacterEllipsis" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Top">
                            <Button x:Name="MinimizeButton" Foreground="{TemplateBinding CurrentForeground}" Style="{StaticResource Minimize}" IsHitTestVisible="False" IsFocusEngagementEnabled="False" IsTabStop="False">
                                <ToolTipService.ToolTip>
                                    <ToolTip Content="Minimize" Placement="Bottom"/>
                                </ToolTipService.ToolTip>
                            </Button>
                            <ToggleButton x:Name="MaximizeButton" Foreground="{TemplateBinding CurrentForeground}" Style="{StaticResource MaxRes}" IsHitTestVisible="False" IsFocusEngagementEnabled="False" IsTabStop="False"/>
                            <Button x:Name="CloseButton" Style="{ThemeResource Close}" Foreground="{TemplateBinding CurrentForeground}" IsHitTestVisible="False" IsFocusEngagementEnabled="False" IsTabStop="False">
                                <ToolTipService.ToolTip>
                                    <ToolTip Content="Close" Placement="Bottom"/>
                                </ToolTipService.ToolTip>
                            </Button>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
