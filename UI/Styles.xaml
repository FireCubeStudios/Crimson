                                        </ObjectAnimationUsingKeyFrames>
                                        <!--  <DoubleAnimation Storyboard.TargetName="BottomBar" Duration="0:0:0.3" Storyboard.TargetProperty="ScaleX" EnableDependentAnimation="True" To="0"/>-->
                                    </Storyboard>
                                </VisualState>

